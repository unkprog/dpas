{
  "dependencies": {
    "Microsoft.NETCore.App": "1.1.0",
    "Microsoft.AspNetCore.Hosting": "1.1.0",
    "dpas.Net.Http.Mvc": "1.0.0-*",
    "dpas.Service.Project": "1.0.0-*",
    "dpas.Service.Server": "1.0.0-*",
    "NETStandard.Library": "1.6.1",
    "Microsoft.Extensions.Logging.Console": "1.1.0"
  },

  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview2-final"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },
  "runtimes": {
    "win10-x64": {}
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "web.config"
    ]
  },
  "scripts": {
    "postcompile": [ "xcopy %project:Directory%/content %project:Directory%/../dpas.Service.Server/content /Y /R /S /E" ]
  }

}
